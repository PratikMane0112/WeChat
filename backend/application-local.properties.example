# Example local development configuration
# Copy this file to 'application-local.properties' and fill in your actual values

# Database configuration
DATABASE_URL=jdbc:postgresql://your-host:5432/your-database?sslmode=require
DB_USERNAME=your_db_username
DB_PASSWORD=your_db_password

# Mail configuration
MAIL_USERNAME=your_email@gmail.com
MAIL_PASSWORD=your_gmail_app_password

# JWT configuration (generate a secure random string of at least 32 characters)
JWT_SECRET=your_jwt_secret_key_here_at_least_32_characters_long

# Development settings
SHOW_SQL=true
SPRING_PROFILES_ACTIVE=development
